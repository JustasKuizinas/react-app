{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": false,
    "sourceMap": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "esnext"],
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}
